  "scripts": {
    "build": "npx tsc",
    "dev": "NODE_ENV=development npx ts-node-dev src/server",
    "start": "node build/server",
    "test": "npx jest -i",
    "clear": "rm logs/*",
    "test-watch": "npx jest --watch",
    "deploy": "NODE_ENV=alpha docker-compose up --detach --build",
    "deploy-alpha": "yarn build && NODE_ENV=alpha docker-compose up --detach --build",
    "deploy-production": "yarn build && NODE_ENV=production docker-compose up --detach --build"
  },
  "devDependencies": {
